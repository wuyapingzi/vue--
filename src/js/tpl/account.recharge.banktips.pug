- var bankTipsData = _bankTips[bankCardIdx]
- var name = bankTipsData.name || ''
- var single = bankTipsData.single || ''
- var day = bankTipsData.day || ''
- var netPassSingle = bankTipsData.netPassSingle || ''
- var netPassDay = bankTipsData.netPassDay || ''
- var certificateSingle = bankTipsData.certificateSingle || ''
- var certificateDay = bankTipsData.certificateDay || ''
