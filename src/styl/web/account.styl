.uc
  $uc-border-color = #efefef
  $uc-header-border-color = #dddddd
  $uc-loan-list-tab-header-bg = #f7f9fc
  $uc-loan-list-tab-header-border = #e7ebf0
  $uc-loan-panel-active-color = #f69907
  $uc-setting-grey-color = #B3B3B3
  $uc-setting-title-active-color  = #76B72C
  $uc-setting-link-color = #73ADFF
  $uc-setting-bank-top-border = #f2f2f2
  $uc-setting-bank-card-border = #cecece
  $uc-setting-message-color = #a0a0a0
  $uc-edit-success-color = #6fbe4c
  $uc-mobile-active-color = #4398f5
  $uc-mobile-success-color = #7ed321
  $uc-mobile-success-btn-bg = #336cbb
  $uc-mobile-success-btn-bg-active = #427bc9
  $uc-home-panel-line-color = #e5e5e5
  $uc-home-border-line = #f4f4f4
  $uc-red-color = #F55151
  $uc-trad-header-type-bg = #eff1f4
  $uc-charge-bank-tips = #dee0e3
  $uc-riskvail-checked = #3282FF
  $uc-riskvail-content-border= #E6e6e6
  $uc-riskvail-uncheck-out = #ECECEC
  $uc-riskvail-uncheck-in  = #c9c9c9
  $uc-riskvail-check-out = #ABCCFF
  $uc-riskvail-check-in  = #3B88FF
  $uc-riskvail-red =#ED3E40
  $uc-riskvail-result-color = #3B88FF
  $grey-color = #666
  $uc-landing-title = #4A4A4A
  $uc-landing-content = #666666
  $uc-item-propertys-color = #FF933C 
  $uc-prcessbar-begin = #F9DC81
  $uc-prcessbar-end = #FFA746
  $uc-loan-pannel-header = #474D5C
  $uc-loan-pannel-item = #D8d8d8
  $uc-poper = #73A3FF
  $uc-message-checkbox = #DDDDDD
  $uc-setting-list-bg = #fbfbfb
  $uc-mobile-processbar = #dbdbdb 
  $uc-mobile-processbar-act = #A4CD74 
  $uc-form-label = #6A6A6A
  $uc-header-link-color = #73A3FF
  $uc-header-title-line-color = #F55151
  $uc-table-head-color = #f9f9fb
  $uc-tabel-border = #EAEAEA
  $uc-error-color = #A94442
  $uc-error-bg-color = #F2DEDE
  $uc-withdraw-list-title-color = #F9F9FB
  $unbind-btn-color = #73A3FF
  $unbind-model-btn-color = #ed3e40
  $uc-invite-list-bg = #F5F5F9
  $uc-invite-wx-border-color = #E3E3E3
  $uc-invite-wx-bg = #F3F3F3
  $uc-copy-success-color = #73ADFF
  &-riskvail
    background: $white-color
    padding: 30px
    margin: 20px auto 40px
    h4
      font-size: $font-size-h3
      color: $grey-color-dark
      text-align: center
      margin-top: 30px
      margin-bottom: 0;
    &-skip
      font-size: $font-size-middle
      color: $uc-riskvail-checked
      line-height: 1
      float: right
      cursor: pointer
    &-content
      margin-top: 16px
      width: 100%
      &-info
        font-size: $font-size-base
        color: $grey-color
        border-top: 1px solid $uc-riskvail-content-border
        padding-top: 10px
        margin: 0 auto
        margin-bottom: 26px
    &-question
      margin-bottom: 30px
    &-question-title
      font-size: $font-size-middle
      color: $grey-color-dark
      line-height: 26px
      margin-bottom: 3px
      span
        margin-right: 22px
        display: inline-block
    .hasNoCheck
      color: $uc-riskvail-red
    &-question-option
      font-size: $font-size-base
      line-height: 15px
      color: $grey-color
      margin-left: 33px
      list-style: none
      label
        margin: 0
        vertical-align: top
        font-weight: normal
        padding-top: 10px
        cursor: pointer
      input
        display: none
      i
        display: inline-block
        position: relative
        margin-right: 12px
        font-style: normal
        padding-left: 20px
        &:before
          content: ''
          height: 10px
          width: 10px
          border-radius: 50%
          position: absolute
          left: 0
          top: 2px
          display: inline-block
          background: $uc-riskvail-uncheck-out
        &:after
          content: ""
          display: inline-block
          height: 4px
          width: 4px
          border-radius: 50%
          background: $uc-riskvail-uncheck-in
          position: absolute
          top: 5px
          left: 3px
      input:checked+i
        color: $uc-riskvail-checked
        &:before
          background: $uc-riskvail-check-out
        &:after
          background: $uc-riskvail-check-in
    &-btn-submit
      margin: 80px auto 47px
      display: block
      width: 300px
    &-modal
      height: 270px
      width: 500px
      background: $white-color
      border-radius: 4px
      padding-top: 60px
      .riskmodal-h3
        font-size: $font-size-h3
        text-align: center
        line-height: 1
        margin-bottom: 20px
      .riskmodal-info
        font-size: $font-size-huge
        color: $grey-color-dark
        margin-bottom: 20px
        line-height: 1
        text-align: center
        span
          color: $uc-riskvail-result-color
      .riskmodal-btn
        margin: 0 auto;
        margin-top: 50px
        text-align: center
        font-size: $font-size-h4
        &-cancel,&-skip
          height: 40px
          width: 150px
          line-height: 40px
          border-radius: 4px
          background: $uc-riskvail-red
          color: $white-color
          display: inline-block
          cursor: pointer
        &-skip
          background: $white-color
          border: 1px solid $uc-riskvail-red
          color: $uc-riskvail-red
          margin-left: 20px
        &-comfirm
          height: 40px
          line-height: 40px
          border-radius: 4px
          width: 300px
          background: $uc-riskvail-red
          color: $white-color
          cursor: pointer
          display: inline-block

  &-panel
    border: 1px solid $uc-border-color
    margin: 20px auto 
    background: $white-color
    padding: 28px 0
    clearfix()
    .uc
      &-panel
        &-amount
          width: 710px
          background: $white-color
          height: 118px
          float: left
          margin-right: 10px
          padding: 30px 0
        &-item
          width: 33%
          float: left
          text-align: center
          line-height: 1
          border-right: 1px solid $uc-loan-pannel-item
          &.last
            border: none
        &-header
          color: $gray-txt-black
          img
            display: inline-block
            vertical-align: middle
            margin-right: 10px
          &-plan  
          &-loan
            color: $uc-loan-pannel-header 
            font-size: $font-size-h4
            margin-bottom: 20px 
        &-body
          font-size: $font-size-h2
          margin-bottom: 12px
          line-height: 1
          color: $uc-loan-pannel-header 
        &-active
          color: $uc-red-color
  &-home
    &-panel
      height: 60px
      padding: 10px 30px
      color: $gray-txt-black
      background: $white-color
      clearfix()
    &-coupon
      float: left
      width: 375px
      height: 118px
      background: $white-color
      margin: 0
      a
        color: $uc-setting-link-color
    &-asset
      &-count
        border: 1px solid $uc-home-border-line
        border-top: none
        overflow: hidden
        .uc-panel
          border-top: none
          padding: 30px 0
          margin: 0
          margin-right: 10px
          border: 1px solid $uc-home-border-line
          width: 710px
          float: left
        .uc-coupon
          float: right
          background: $white-color
          width: 375px
          height: 118px
          margin-top: 0
          padding: 30px
          &-total
            text-align: center
            color: $gray-txt-black
            .coupon-count
              display: inline-block
              padding-left: 40px
              background: url('/img/product-detail-coupon.png') no-repeat 0 12px
            span
              font-size: $font-size-h2
              color: $btn-primary-color
              margin: 0 10px
              vertical-align: -6px
            a
              color: $uc-setting-link-color
        .uc-panel-item
          border: none 
      &-info
        border: 1px solid $uc-home-border-line
        border-top: none
        overflow: hidden
        margin-top: 20px
        height: 325px
        clearfix()
        &-header
          width: 710px
          height: 100%;
          padding-bottom: 30px
          margin-right: 10px
          float: left
          background: $white-color
          clearfix()
          h4
            margin: 20px 0 0 63px
            position: relative
            &:before
              content: ''
              height: 100%
              width: 3px
              background: $uc-red-color
              position: absolute
              left: -13px
              top: -1px
        .item-info
          &-header
            width: 205px
            float: left
            margin-top: 11px;
          &-body
            padding-top: 68px
            dl
              overflow: hidden
              clearfix()
            dt,dd
              float: left
              line-height: 1
            dt
              font-weight: normal
              position: relative
              padding-left: 16px
              color: $uc-mobile-active-color
              line-height: 1
              &:before
                width: 8px
                height: 8px
                display: block
                content: ''
                border-radius: 2px
                background: $uc-mobile-active-color
                position: absolute
                left: 0
                top: 3px
            .item-info-plan
              color: $uc-red-color
              &:before
                background: $uc-red-color
            .item-info-plan-amount
              display: inline-block
              width: 114px
            .item-info-plan-earnings
              width: 100px
              text-align: left
              display: inline-block
            .item-info-userfund
              color: $uc-loan-panel-active-color
              &:before
                background: $uc-loan-panel-active-color
            dd
              margin-left: 20px
              color: $gray-txt-black
              span
                color: $gray-txt-light
                margin: 0 20px 0 6px
                text-align: left
          &-chart
            width: 210px
            height: 200px
          &-asset
            color: $gray-txt-black
            margin-top: -18px
            text-align: center
            span
              color: $gray-txt-light
              margin-left: 6px
              display: block
              margin-top: 3px
        &-body
          float: right
          width: 375px
          height: 100%;
          padding: 20px 40px
          background: $white-color
          h4
            margin-top: 0px
            padding-left: 13px
            position: relative
            &:before
              content: ''
              position: absolute
              height: 100%
              width: 3px
              background: $uc-red-color
              left: 0
    &-item
      margin: 40px auto 20px
      text-align: center
      &-body
        font-size: $font-size-h2
        margin-bottom: 10px
        line-height: 30px
        color: $uc-loan-pannel-header
      &-header
        color: $gray-txt-black
    &-userfund
      margin-top: 30px
      overflow: hidden
      clearfix()
      dl
        width: 100%
        text-align: center
        overflow: hidden
        margin-bottom: 15px
      dt
        color: $gray-txt-black
        font-weight: normal
        font-size: $font-size-base
        float: left
        margin-top: 4px
        line-height: 1
      dd
        float: right 
        font-size: $font-size-base 
        line-height: 1
      .uc-home-userfund-last
        border-right: none
    &-recharge-withdraw
      margin-top: 15px
      a
        width: 48%
        height: 44px
        line-height: 44px
        text-align: center
        display: inline-block
        border: 1px solid $uc-home-panel-line-color
        border-radius(2px)
        cursor: pointer
      .uc-home-withdraw
        margin-left: 10px
        color: $gray-txt-black
      .uc-home-recharge
        background: $uc-red-color
        color: $white-color
        border-color: $uc-red-color
    &-tradorder
      padding: 30px
      margin: 20px 0 40px
      background: $white-color
      h4
        margin-bottom: 30px
        padding-left: 33px
        position: relative
        &:before
          position: absolute
          content: ''
          height: 18px
          width: 3px 
          background: $uc-red-color
          left: 20px
        clearfix()
        a
          float: right
          padding-right: 20px 
          color: $uc-mobile-active-color
          font-size: $font-size-base  
      th
        background: $uc-loan-list-tab-header-bg 
        line-height: 60px
        color: $gray-txt-black
        font-weight: normal
        text-align: center
      .trade-time
        text-align: left
        padding-left: 20px
        width: 290px
      .balance
        text-align: right 
        padding-right: 20px 
        width: 210px
      .abstract
        display: inline-block
        position: relative
        margin-bottom: -1px
        margin-left: 4px
        height: 12px
        width: 12px
        background: url('img/uc-opratehint.png')
        &:hover .uc-trad-tips
          display: block
      td
        padding: 20px 0
        text-align: center  
        border-bottom: 1px solid $uc-tabel-border
      .uc-trad-tips
        display: none
        width: 230px
        position: absolute
        padding: 10px
        bottom: -30px
        left: 15px
        text-align: left
        border-radius: 4px
        color: $grey-color  
        background: $body-bg
        &:before
          position: absolute
          left: -8px
          bottom: 30px
          content: ''
          display: block
          border-top: 4px solid transparent
          border-bottom: 4px solid transparent
          border-left: 4px solid transparent
          border-right: 4px solid $body-bg
        a
          color: $uc-setting-link-color
      .item
        &-spac
          background: $white-color
        &-plus
          color: $up-color
        &-minus
          color: $down-color
  &-info
    float: left
    width: 500px
    line-height: 1
    font-size: $font-size-base
    img
      margin-right: 20px
      height: 38px
      width 38px
      float: left
    span
      float: left
      margin-top: 14px
      color: $grey-color-dark 
  //coupon
  &-coupon
    $font-size-rate = 42px
    margin-top: 20px
    padding: 0 0 40px
    .uc-title
      padding-left: 50px
      a
        font-size: $font-size-base
        line-height: 22px
        float: right
        color: $uc-header-link-color
        cursor: pointer
    .get-coupon
      display: block
      text-align: center
      color: $uc-header-link-color
      font-size: $font-size-middle 
    &-list
      width: $container-width
      background-color: $white-color
      padding-top: 30px
      .coupon
        &-nav
          a:hover
            color: $buy-botton-color
        &-content
          border-top: solid 1px $uc-message-checkbox
          padding-bottom: 40px
        &-list
          overflow: hidden
          padding: 70px 30px 34px
          clearfix()
          .coupon-left
            margin-left: 0
          .pagination
            clearfix()
        &-item
          width: 330px
          float: left
          height: 140px
          color: $white-color
          position: relative
          padding: 20px 0 20px 10px
          margin: 0 auto 40px 24px
          .item-header
            font-size: $font-size-h3
            margin-bottom: 26px
            line-height: 22px
          .coupon-use
            position: absolute
            top: 0
            right: 0
            width: 33px
            height: 140px
            border-left: dashed 1px $white-color
            padding: 30px 9px
          .coupon-invalid
            padding: 40px 9px
          a
          p
            width: 14px
            display: inline-block
            line-height: 20px
            color: $white-color
        &-title
          font-size: $font-size-base
          float: right
          margin: -4px 43px auto auto
        &-duration
        &-use-condition
        &-product
          font-size: $font-size-h6
          margin-bottom: 8px
          line-height: 12px
        &-rate
          float: right
          font-size: $font-size-rate
          margin: -50px 43px auto auto
          span
            font-size: $font-size-middle
        &-deduction
          background: url('/img/coupon-discount.png')
        &-moneyoff
          background: url('/img/coupon-money-off.png')
        &-used
          background: url('/img/coupon-used.png')
      .active
        color: $buy-botton-color
        border-bottom: solid 2px $buy-botton-color
    .pagination
      margin-top: 6px
  &-aptitude
    float: right
    margin: 6px -10px 0px 0px
    height: 30px
    a
      cursor: pointer
    span
      size(28px)
      display: inline-block
      padding: 0 28px
      background: url('/img/icon-account-home.png') no-repeat center center
      position: relative
      &:after
        content: ''
        position: absolute
        width: 1px
        height: 10px
        top: 10px
        right: 0px
        background-color: $uc-home-panel-line-color
    .uc-aptitude-last
      &:after
         content: ''
         width: 0
    .uc-aptitude-mobile
      background-position: 18px -23px
      cursor: pointer
      &.active
        background-position: 18px 5px
        cursor: default
    .uc-aptitude-tradpwd
      background-position: 18px -79px
      cursor: pointer
      &.active
        background-position: 18px -50px
        cursor: default
    .uc-aptitude-account
      background-position: 18px -128px
      cursor: pointer
      &.active
        background-position: 18px -103px
        cursor: default
    .uc-aptitude-auth
      background-position: 18px -176px
      cursor: pointer
      &.active
        background-position: 18px -150px
        cursor: default
    a
      color: $gray-txt-black
    .active
      color: $uc-mobile-success-btn-bg
  &-last-login-time
    float: left
    margin: 14px
    font-size: $font-size-small
  &-nav
    min-width: $container-width
    background: $white-color
  &-nav-link
    font-size: $font-size-base
    display: inline-block
    padding: 16px 56px 16px 0
    &:hover
      color: $base-red-color
  &-nav-active
    color: $uc-red-color
  &-tabs-table
    padding: 0 30px 
    .disable
      color: $gray-txt-black
  &-product-list
    background: $white-color
    padding-bottom: 20px
    margin-bottom: 40px
    .dropdown-list
      td
        padding: 0
        border: none
      .dropdown-box
        display: none
    .small-table
      margin: 0 auto 
      width: 100%
      box-shadow: 0 1px 10px 0 rgba(0,0,0,.2)
      td
        border: none
        height: 60px
        line-height: 60px 
    .add-border
      box-shadow: 0 -1px 10px 0 rgba(0,0,0,.2)
      td
        border-bottom: none
    .release
      cursor: pointer
      display: inline-block
      border: 6px solid transparent
      border-top-color: $uc-poper
      margin-bottom: -4px
    .collect 
      cursor: pointer
      display: inline-block
      border: 6px solid transparent 
      border-bottom-color: $uc-poper 
      margin-bottom: 3px       
    &-tab
      margin-top: 20px   
      .arrow-point
        width: 18px
      th
        color: $gray-txt-black
        background: $uc-table-head-color
        font-weight: normal
        padding: 20px 5px
        text-align: center
      .first
        text-align: left
        padding-left: 22px
        width: 230px
      .rate
        width: 130px
      .income
        width: 100px
      .endtime
        width: 140px
      td
        padding: 20px 0
        text-align: center
        border-bottom: 1px solid $uc-tabel-border
        p
          padding: 4px 0
          margin: 0
        a
          color: $uc-poper
      .item-kesy
        color: $blue-linghter-color
      .pagination
        display: inline-block
      .transfered
        display: inline-block
        height: 22px
        width: 24px
        vertical-align: bottom
        margin-right: 3px
        background: url('img/debt-transfer.png')
      .none-transfer
        display: inline-block
        height: 22px
        width: 24px
        vertical-align: bottom
        margin-right: 3px 
      .transferable
        color: $gray-txt-black 
      .page
        border-bottom: none
    &-error
      min-height: 450px
      padding: 30px 30px
      font-size: $font-size-h5
      color: $gray-txt-black
    .uc-plan-earn
      color: $uc-loan-panel-active-color
  &-title
    padding: 16px 20px
    font-size: $font-size-middle
    h4
      display: inline-block
      border-left: 3px solid $uc-header-title-line-color
      margin: 0
      padding-left: 13px
  &-form
    width: 540px
    margin: 50px auto 40px
    padding: 0 60px
    .form-group
      padding-bottom: 5px
      position: relative
    .charge-btn
      width: 300px
      line-height: 32px
      margin-top: 30px
    .control-label
      width: 120px
      line-height: 30px
      padding-right: 20px
      text-align: right
      float: left
      font-weight: normal
      font-size: $font-size-base
      color: $uc-form-label
    .control-content
      margin-left: 120px
      position: relative
    .form-control
      height: 40px
      padding: 10px 12px
      border-radius(4px)
    .btn-secure-submit
      width: 100%
      height: 40px
      line-height: 40px
      padding: 0
      margin-top:10px
      background: $uc-red-color
      &:disabled
        background: $border-color
      &:active
        background: $uc-red-color
    &-password-success
      min-height: 450px
      font-size: $font-size-h3
      color: $uc-edit-success-color
      text-align: center
      line-height: 450px
    .img-validate
      width: 100px
      height: 44px
      margin-left: 10px
  &-unbindcard
    background: $white-color
    margin: 20px auto 40px
    padding: 30px 20px
    border: 1px solid $uc-border-color
    min-height: 510px
    _bgp()
      background-position: arguments
    $code = '0160' '0150' '0210' '0220' '5006' '5002' '0201' '5007' '5004' '5003' '5012' '5000' '5001' '5008' '5010' '5009' '5005' '0170' '5011' '5013' '1114' '0250' '5186'
    &-from
      width: 420px
      height: 400px
      margin: 30px auto 40px
      padding: 0
      .form-line
        line-height: 30px
      input
        width: 300px
    &-findpass
      display: inline-block
      position: absolute
      right: -60px
      bottom: 10px
      color: $unbind-btn-color
      font-size: $font-size-base
      &:hover
        color: $unbind-btn-color
    .help-inline
      bottom: -15px
      left: 120px 
      font-size: $font-size-base
    .item-buy-icon
      height: 30px
      width: 30px 
      display: inline-block 
      vertical-align: bottom
      margin-right: 5px
    .bank-logo
      // background-size: 30px auto
      background: url('/img/bank_logo_small.png') no-repeat -2px 0
    for code, index in $code
      .buy-bank-logo-{code}
        _bgp(-2px, (-46*index)px)
    .icon-unbind-error
      background: url('/img/buy-result-error.png') no-repeat center 70px
  &-unbindresult
    min-height: 420px
    text-align: center
    padding-top: 200px;
    background: url('/img/buy-result-success.png') no-repeat center 70px
    .uc-unbindresult-margin
      margin-top:20px
    &-myaccount
      border: 1px solid $buy-botton-color
      color: $uc-red-color
      background: $white-color
      cursor: pointer
      &:hover
