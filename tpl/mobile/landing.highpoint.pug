  img(src='/img/mobile/landing-raise-title-enjoy.png').landing-raise-enjoy
  .landing-raise-wrap
    img(src='/img/mobile/landing-raise-list-title-one.png').landing-raise-list-title
    .landing-raise-wrap.landing-raise-list
      img(src='/img/mobile/landing-raise-one-one.png', data-path='/plan/detail', data-productid=_dataList[0]).J_jump-to-app.landing-raise-list-first
      img(src='/img/mobile/landing-raise-one-two.png', data-path='/plan/detail', data-productid=_dataList[1]).J_jump-to-app
      img(src='/img/mobile/landing-raise-one-three.png', data-path='/plan/detail', data-productid=_dataList[2]).J_jump-to-app.landing-raise-list-last
  .landing-raise-wrap
    img(src='/img/mobile/landing-raise-list-title-two.png').landing-raise-list-title
    .landing-raise-wrap.landing-raise-list
      img(src='/img/mobile/landing-raise-two-one.png', data-path='/plan/detail', data-productid=_dataList[3]).J_jump-to-app.landing-raise-list-first
      img(src='/img/mobile/landing-raise-two-two.png', data-path='/plan/detail', data-productid=_dataList[4]).J_jump-to-app
